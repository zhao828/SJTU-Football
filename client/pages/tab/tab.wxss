/* pages/tab/tab.wxss */
.swiper{
  display: flex;
  flex-direction: column;
  height: 100vh;
  background-color: #0F1E3D;
}

.selectors{
  display: flex;
  flex-direction: column;
 
}
.font{
  color:#AA936C;
  font-size: 200%;
  font-weight:bold;

}
.page{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100vh;
  justify-content: space-evenly
 
}

.selector{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  
  
}
.item{
  width: 200rpx;
  color: #AA936C;
  text-align: center;
}
.input{
  width: 200rpx;
  text-align: center;
  color:  #AA936C;
  border-bottom: 2rpx solid #AA936C
}
.button{
  color:#AA936C;
  background: #0F1E3D;
  border: 2px solid #AA936C; 
  border-radius: 2px;
}
